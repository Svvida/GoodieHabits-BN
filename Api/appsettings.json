{
  "ConnectionStrings": {
     "DefaultConnection": "Server=TemplateHost;Database=MyDatabase;Trusted_Connection=True;"
    },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "YourIssuer",
    "Audience": "YourAudience",
    "AccessToken": {
      "Key": "YourSecretKey",
      "ExpirationMinutes": "60"
    },
    "RefreshToken": {
      "Key": "YourSecretRefreshTokenKey",
      "ExpirationDays": "60"
    }
  },
  "AllowedHosts": "*",
  "SeedData": false
}
